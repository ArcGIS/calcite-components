{
  "name": "@esri/calcite-components",
  "version": "1.0.0-beta.31",
  "description": "Web Components for Esri's Calcite Design System.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/components.d.ts",
  "unpkg": "dist/calcite/calcite.js",
  "collection:main": "dist/collection/index.js",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/",
    "hydrate/"
  ],
  "scripts": {
    "demoPageSync": "ts-node ./support/demoPageSync.ts",
    "build": "npm run copy-icons && stencil build",
    "copy-icons": "cpy ./node_modules/@esri/calcite-ui-icons/js/*.json ./src/components/calcite-icon/assets",
    "deps:update": "updtr --ex jest-runner @types/jest @types/puppeteer jest jest-cli puppeteer && git add package*.json && git commit -q -m \"build(deps): bump versions\"",
    "docs": "concurrently --kill-others --raw \"npm run build:docs && build-storybook --static-dir ./__docs-temp__ --output-dir ./docs\"  \"ts-node ./support/tempDocDirCleaner.ts\"",
    "docs:preview": "concurrently --raw \"npm run build:docs && start-storybook --static-dir ./__docs-temp__\" \"ts-node ./support/tempDocDirCleaner.ts\"",
    "lint": "concurrently npm:lint:*",
    "lint:ts": "tslint --project tsconfig-tslint.json && eslint --ext .ts,.tsx .",
    "lint:styles": "stylelint \"src/**/*.scss\"",
    "start": "concurrently --kill-others --raw \"npm run copy-icons & npm run demoPageSync\" \"tsc --project ./tsconfig-demos.json --watch\" \"stencil build --dev --watch --serve\"",
    "test": "npm run copy-icons && stencil test --spec --e2e",
    "test:headed": "npm run copy-icons && stencil test --spec --e2e --devtools",
    "test:watch": "npm run copy-icons && stencil test --spec --e2e --watch",
    "test:watchAll": "npm run copy-icons && stencil test --spec --e2e --watchAll",
    "posttest": "stencil build --prerender",
    "prettier": "concurrently npm:prettier:*",
    "prettier:ts": "prettier --write \"src/**/*.ts?(x)\"",
    "prettier:styles": "prettier --write \"src/**/*.scss\"",
    "previewChangelog": "standard-version --no-skip.bump --silent && cat CHANGELOG.md && git reset --hard --quiet",
    "previewChangelog:latest": "npx touch __CHANGELOG-PREVIEW-TEMP__ && standard-version --no-skip.bump --release-count 1 --infile __CHANGELOG-PREVIEW-TEMP__ --same-file --silent && cat __CHANGELOG-PREVIEW-TEMP__ && rimraf __CHANGELOG-PREVIEW-TEMP__ && git reset --hard --quiet",
    "prerelease:prepare": "npm run build",
    "release:prepare": "npm version prerelease --preid=beta --no-git-tag-version ",
    "release:publish": "./support/release.sh",
    "prestorybook": "npm run build",
    "storybook": "start-storybook -s ./dist",
    "prebuild-storybook": "npm run build",
    "build-storybook": "build-storybook -s ./dist -o .docs",
    "deploy-storybook": "storybook-to-ghpages",
    "deploy": "npm-run-all build deploy-storybook",
    "util:cleanTestedBuild": "npm ci && npm test && npm run build",
    "util:deployNextFromExistingBuild": "npm run util:prepNextFromExistingBuild && npm run util:pushTags && npm run util:publishNext",
    "util:prepNextFromExistingBuild": "standard-version --no-skip.bump --no-skip.commit --no-skip.tag --skip.changelog --prerelease beta",
    "util:pushTags": "git push --follow-tags",
    "util:publishNext": "npm publish --tag next",
    "version": "npm run util:cleanTestedBuild && npm run docs && standard-version && git add ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Esri/calcite-components.git"
  },
  "dependencies": {
    "sortablejs": "1.10.2",
    "@esri/calcite-colors": "2.1.0",
    "@a11y/focus-trap": "git+https://github.com/patrickarlt/focus-trap.git#conditional-define-build",
    "@popperjs/core": "~2.3.3"
  },
  "devDependencies": {
    "@esri/calcite-base": "^1.2.0",
    "@esri/calcite-ui-icons": "~3.2.0",
    "@stencil/core": "~1.11.3",
    "@stencil/eslint-plugin": "0.3.1",
    "@stencil/sass": "^1.1.1",
    "@stencil/postcss": "1.0.1",
    "@stencil/state-tunnel": "^1.0.1",
    "@storybook/addon-a11y": "^5.3.19",
    "@storybook/addon-backgrounds": "~5.3.17",
    "@storybook/addon-docs": "5.3.19",
    "@storybook/addon-centered": "~5.3.17",
    "@storybook/addon-knobs": "~5.3.17",
    "@storybook/addon-notes": "~5.3.17",
    "@storybook/cli": "~5.3.17",
    "@storybook/html": "~5.3.17",
    "@storybook/storybook-deployer": "~2.8.3",
    "@types/dedent": "0.7.0",
    "@types/jest": "24.0.21",
    "@types/jest-axe": "^3.2.1",
    "@types/puppeteer": "1.20.2",
    "@types/rimraf": "3.0.0",
    "@types/webpack": "4.41.19",
    "@types/sortablejs": "1.10.4",
    "@types/autoprefixer": "9.7.2",
    "@types/lodash-es": "4.17.3",
    "@typescript-eslint/eslint-plugin": "3.5.0",
    "@typescript-eslint/parser": "3.5.0",
    "autoprefixer": "9.8.4",
    "awesome-typescript-loader": "5.2.1",
    "axe-core": "~3.5.2",
    "babel-loader": "~8.1.0",
    "chalk": "4.1.0",
    "chokidar": "3.4.0",
    "concurrently": "5.2.0",
    "dedent": "0.7.0",
    "eslint": "7.4.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.3",
    "cpy-cli": "^3.1.0",
    "gh-release": "^3.5.0",
    "github-release-from-cc-changelog": "2.2.0",
    "husky": "4.2.3",
    "jest": "24.9.0",
    "jest-axe": "~3.4.0",
    "jest-cli": "24.9.0",
    "jest-runner": "24.9.0",
    "lint-staged": "10.1.0",
    "lodash-es": "^4.17.15",
    "package": "1.0.1",
    "posthtml": "0.13.1",
    "rimraf": "3.0.2",
    "standard-version": "8.0.0",
    "stylelint": "13.6.1",
    "stylelint-config-recommended-scss": "4.2.0",
    "stylelint-scss": "3.18.0",
    "ts-jest": "26.1.1",
    "ts-node": "8.10.2",
    "tslint": "6.1.2",
    "tslint-config-prettier": "1.18.0",
    "tslint-stencil": "1.0.1",
    "npm-run-all": "4.1.5",
    "prettier": "~2.0.2",
    "puppeteer": "2.1.0",
    "storybook": "~5.3.17",
    "updtr": "3.1.0",
    "workbox-build": "~5.1.2"
  },
  "license": "UNLICENSED"
}
